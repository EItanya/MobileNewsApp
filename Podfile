target 'MobileNewsApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MobileNewsApp
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'
    pod 'Parse'
    pod 'ParseFacebookUtilsV4'
    pod 'M13Checkbox'
    pod 'SkyFloatingLabelTextField', '~> 2.0.0'
    pod 'MGSwipeTableCell'
    pod 'AWSCore'
    pod 'AWSS3'
    pod 'BBBadgeBarButtonItem'
    pod "SwiftSpinner"
    
  target 'MobileNewsAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MobileNewsAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
